{
  "name": "shiritorius",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preprocess": "ts-node --files src/preprocess.ts",
    "dev": "parcel serve src/index.html --open",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "test": "jest",
    "build": "parcel build src/index.html --experimental-scope-hoisting --public-url .",
    "predeploy": "run-s type-check lint test build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/request": "^2.48.8",
    "@types/tar": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "command-line-args": "^5.2.0",
    "command-line-usage": "^6.1.1",
    "csv-parse": "^5.0.4",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "gh-pages": "^3.2.3",
    "iconv-lite": "^0.6.3",
    "jest": "^27.4.7",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-json-url-loader": "^0.1.2",
    "prettier": "^2.5.1",
    "react": ">=16.8.0",
    "react-dom": "^17.0.2",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "tar": "^6.1.11",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
